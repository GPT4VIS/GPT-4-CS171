        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;
&lt;p&gt;&lt;span&gt;Which jQuery code can update &lt;/span&gt;&lt;em&gt;&lt;strong&gt;&lt;span style="text-decoration: underline;"&gt;the content of the div element (ID: "new-content")&lt;/span&gt;&lt;/strong&gt;&lt;/em&gt;&lt;span&gt; to "CS 171" without changing any other elements?&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;span&gt;HTML document:&lt;/span&gt;&lt;/i&gt;&lt;/p&gt;
&lt;pre&gt;&lt;strong&gt;&amp;lt;body&amp;gt;&lt;br&gt;&lt;/strong&gt;&lt;strong&gt;  &amp;lt;div class="container"&amp;gt;&lt;br&gt;&lt;/strong&gt;&lt;strong&gt;    &amp;lt;h1&amp;gt;Visualization&amp;lt;/h1&amp;gt;&lt;br&gt;&lt;/strong&gt;&lt;strong&gt;    &amp;lt;div id="new-content"&amp;gt;&amp;lt;/div&amp;gt;&lt;br&gt;&lt;/strong&gt;&lt;strong&gt;  &amp;lt;/div&amp;gt;&lt;br&gt;&lt;/strong&gt;&lt;strong&gt;&amp;lt;/body&amp;gt;&lt;/strong&gt;&lt;/pre&gt;
&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Multiple">
            <render_choice>
              <response_label ident="66325">
                <material>
                  <mattext texttype="text/plain">$(".container").html('&lt;div class="new-content"&gt;CS 171&lt;/div&gt;');</mattext>
                </material>
              </response_label>
              <response_label ident="82886">
                <material>
                  <mattext texttype="text/plain">$(".container #new-content").html('CS 171');</mattext>
                </material>
              </response_label>
              <response_label ident="38371">
                <material>
                  <mattext texttype="text/plain">$(".new-content").html('CS 171');</mattext>
                </material>
              </response_label>
              <response_label ident="3231">
                <material>
                  <mattext texttype="text/plain">$(".container #new-content").on('CS 171');</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
